title = {{Distributed Recycling of Post-Consumer Plastic Waste in Rural Areas}},
url = {http://www.journals.cambridge.org/abstract{\_}S1946427413002583 https://www.cambridge.org/core/product/identifier/S1946427413002583/type/journal{\_}article},
volume = {1492},
year = {2013}
}

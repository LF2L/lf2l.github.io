publisher = {John Wiley {\&} Sons, Ltd},
title = {{Role Models for Radical Innovations in Times of Open Innovation}},
url = {http://doi.wiley.com/10.1111/j.1467-8691.2007.00451.x},
volume = {16},
year = {2007}
}

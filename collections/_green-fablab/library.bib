issn = {1932-6203},
journal = {PLoS One},
month = {sep},
number = {9},
pages = {e107216},
title = {{Open-Source Syringe Pump Library}},
url = {http://dx.plos.org/10.1371/journal.pone.0107216},
volume = {9},
year = {2014}

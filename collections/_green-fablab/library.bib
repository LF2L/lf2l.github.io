number = {1-2},
pages = {110--119},
title = {{Fabrication of functionally graded reaction infiltrated SiC–Si composite by three-dimensional printing (3DP™) process}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S092150930001282X},

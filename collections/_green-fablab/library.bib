file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Touati et al. - 2011 - The effects of reprocessing cycles on the structure and properties of isotactic polypropyleneCloisite 15A nanocom.pdf:pdf},
isbn = {0141-3910},
issn = {01413910},
journal = {Polym. Degrad. Stab.},
keywords = {Degradation,Nanocomposites,Organoclay,Polypropylene,Reprocessing},
number = {6},
pages = {1064--1073},
publisher = {Elsevier Ltd},
title = {{The effects of reprocessing cycles on the structure and properties of isotactic polypropylene/Cloisite 15A nanocomposites}},

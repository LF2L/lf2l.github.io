@article{Thomas2016a,
abstract = {There are three primary aspects to the economics of additive$\backslash$nmanufacturing: measuring the value of goods produced, measuring the$\backslash$ncosts and benefits of using the technology, and estimating the adoption$\backslash$nand diffusion of the technology. This paper provides an updated estimate$\backslash$nof the value of goods produced. It then reviews the literature on$\backslash$nadditive manufacturing costs and identifies those instances in the$\backslash$nliterature where this technology is cost-effective. The paper then goes$\backslash$non to propose an approach for examining and understanding the societal$\backslash$ncosts and benefits of this technology both from a monetary viewpoint and$\backslash$na resource consumption viewpoint. The final section discusses the trends$\backslash$nin the adoption of additive manufacturing. Globally, there is an$\backslash$nestimated {\$}667 million in value added produced using additive\backslashnmanufacturing, which equates to 0.01 {\%} of total global manufacturing\backslashnvalue added. US value added is estimated as {\$}241 million. Current$\backslash$nresearch on additive manufacturing costs reveals that it is$\backslash$ncost-effective for manufacturing small batches with continued$\backslash$ncentralized production; however, with increased automation distributed$\backslash$nproduction may become cost-effective. Due to the complexities of$\backslash$nmeasuring additive manufacturing costs and data limitations, current$\backslash$nstudies are limited in their scope. Many of the current studies examine$\backslash$nthe production of single parts and those that examine assemblies tend$\backslash$nnot to examine supply chain effects such as inventory and transportation$\backslash$ncosts along with decreased risk to supply disruption. The additive$\backslash$nmanufacturing system and the material costs constitute a significant$\backslash$nportion of an additive manufactured product; however, these costs are$\backslash$ndeclining over time. The current trends in costs and benefits have$\backslash$nresulted in this technology representing 0.02 {\%} of the relevant$\backslash$nmanufacturing industries in the USA; however, as the costs of additive$\backslash$nmanufacturing systems decrease, this technology may become widely$\backslash$nadopted and change the supplier, manufacturer, and consumer$\backslash$ninteractions. An examination in the adoption of additive manufacturing$\backslash$nreveals that for this technology to exceed {\$}4.4 billion in 2020, {\$}16.0$\backslash$nbillion in 2025, and {\$}196.8 billion in 2035, it would need to deviate\backslashnfrom its current trends of adoption.},
author = {Thomas, Douglas},
doi = {10.1007/s00170-015-7973-6},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Thomas - 2016 - Costs, benefits, and adoption of additive manufacturing a supply chain perspective.pdf:pdf},
isbn = {0017001579},
issn = {0268-3768},
journal = {Int. J. Adv. Manuf. Technol.},
keywords = {Additive manufacturing,Manufacturing,Supply chain},
month = {jul},
number = {5-8},
pages = {1857--1876},
publisher = {The International Journal of Advanced Manufacturing Technology},
title = {{Costs, benefits, and adoption of additive manufacturing: a supply chain perspective}},
url = {http://dx.doi.org/10.1007/s00170-015-7973-6 http://link.springer.com/10.1007/s00170-015-7973-6},
volume = {85},
year = {2016}
}
@article{Tunn2019,
abstract = {Combining sustainable consumption with the circular economy concept could help tackle challenges, such as resource scarcity and climate change by reducing resource throughput and increasing cycling of products and materials within the economic system, thereby reducing emissions and virgin material use. To achieve sustainable consumption in a circular economy production and consumption practices need to change. Business models can potentially influence both practices as it defines how a company conducts business and shapes the company-consumer relationship. This paper developed future business models for sustainable consumption through two rounds of semi-structured interviews with experts from academia, industry, and policy. During the first interview round, four business model elements that are important for sustainable consumption were identified: Resource strategy, Revenue model, Consumer effort, and Objective to (decrease/increase) consumption level. Based on these elements, we developed a comprehensive business model framework. Using this framework, experts envisioned future business models for sustainable consumption of clothing during the second interview round. The findings of this study suggest that the most promising business models for sustainable consumption are those that reduce overall consumption levels and consumer effort. Further, we found that a diverse range of business models in the market can potentially enable different customer segments to consume sustainably.},
author = {Tunn, V. S.C. and Bocken, N. M.P. and van den Hende, E. A. and Schoormans, J. P.L.},
doi = {10.1016/j.jclepro.2018.11.290},
issn = {09596526},
journal = {J. Clean. Prod.},
keywords = {Business model,Circular economy,Clothing industry,Expert interview,Sufficiency,Sustainable consumption},
month = {mar},
pages = {324--333},
publisher = {Elsevier Ltd},
title = {{Business models for sustainable consumption in the circular economy: An expert study}},
volume = {212},
year = {2019}
}

@article{Mackay2017a,
abstract = {The failure (maximum) feed velocity in a LulzBot Taz 4 3D printer at various temperatures is determined for three polymers: Acrylonitrile butadiene styrene, poly(lactic acid) (PLA), and a PLA polyhydroxybutyrate copolymer. Through an approximate solution of the energy balance, we develop a model to correlate the dimensionless fiber feed velocity (represented by a P{\'{e}}clet number) with a dimensionless temperature. Using these dimensionless parameters, all polymers fall onto the same curve. However, when molten polymer is forced through a small nozzle to enable 3D printing, this curve also depends on another parameter: Nozzle diameter. Our model does not account for this parameter because it does not consider hydrodynamics due to the complexity of the coupled energy and momentum balances. Thus, we modify the P{\'{e}}clet number to account for hydrodynamics and produce a satisfactory master curve for all diameters and polymers. Our dimensionless numbers require determining the polymer thermal and rheological propert...},
author = {Mackay, Michael E. and Swain, Zachary R. and Banbury, Colby R. and Phan, David D. and Edwards, David A.},
doi = {10.1122/1.4973852},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Mackay et al. - 2017 - The performance of the hot end in a plasticating 3D printer.pdf:pdf},

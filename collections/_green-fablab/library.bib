pages = {694--704},
publisher = {Elsevier},
title = {{Sustainability of additive manufacturing: An overview on its energy demand and environmental impact}},
url = {https://doi.org/10.1016/j.addma.2018.04.022 https://linkinghub.elsevier.com/retrieve/pii/S2214860417302646},

journal = {Energy Res. Soc. Sci.},
keywords = {Circular economy,Expectations,Narratives,Transition},
number = {October 2016},
pages = {60--69},
publisher = {Elsevier},
title = {{Narrating expectations for the circular economy: Towards a common and contested European transition}},
url = {http://dx.doi.org/10.1016/j.erss.2017.05.006},
volume = {31},
year = {2017}
}

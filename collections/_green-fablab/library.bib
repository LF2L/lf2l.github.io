journal = {J. Mech. Phys. Solids},
keywords = {3D printing,Bond exchange reaction,Chemomechanics modeling,Dual-stage polymerization,Reprocessable thermoset},
pages = {168--186},
title = {{Chemomechanics of dual-stage reprocessable thermosets}},
volume = {126},
year = {2019}
}

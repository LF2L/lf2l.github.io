publisher = {Elsevier Ltd},
title = {{Identification and quantification of oligomers as potential migrants in plastics food contact materials with a focus in polycondensates – A review}},
url = {http://dx.doi.org/10.1016/j.tifs.2016.01.018 https://linkinghub.elsevier.com/retrieve/pii/S0924224416000212},

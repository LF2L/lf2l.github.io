issn = {18125662},
journal = {J. Appl. Sci.},
keywords = {In-house recycling,Plastic waste management,Plastics recycling,Recycling process},
month = {apr},
number = {7},
pages = {1030--1035},
publisher = {Asian Network for Scientific Information},
title = {{Recycling trends in the plastics manufacturing and recycling companies in Malaysia}},
volume = {7},

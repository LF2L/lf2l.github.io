doi = {10.1007/s12008-017-0412-1},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Markou et al. - 2017 - A methodological proposal to link Design with Additive Manufacturing to environmental considerations in the Early.pdf:pdf},

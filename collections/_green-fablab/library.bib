journal = {Resour. Conserv. Recycl.},
keywords = {Motivation mechanism,Organic waste,Public participation,Separation at the source},
pages = {77--90},
publisher = {Elsevier B.V.},
title = {{Enhancing organic waste separation at the source behavior: A case study of the application of motivation mechanisms in communities in Thailand}},
url = {http://dx.doi.org/10.1016/j.resconrec.2014.12.002},
volume = {95},

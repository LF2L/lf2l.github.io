publisher = {Elsevier Ltd},
title = {{Online versus face-to-face collaboration in the context of a computer-supported modeling task}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S074756321400243X},
volume = {37},
year = {2014}
}

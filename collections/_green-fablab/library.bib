journal = {Resour. Conserv. Recycl.},
keywords = {Circular economy,Distributed manufacturing,Distributed recycling,Energy conservation,Polymer recycling,Sustainable development},
month = {jan},
number = {July 2017},
pages = {48--58},
publisher = {Elsevier},
title = {{Tightening the loop on the circular economy: Coupled distributed recycling and manufacturing with recyclebot and RepRap 3-D printing}},
url = {https://doi.org/10.1016/j.resconrec.2017.09.023 https://linkinghub.elsevier.com/retrieve/pii/S0921344917303014},

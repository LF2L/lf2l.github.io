month = {sep},
pages = {940--952},
publisher = {Elsevier Ltd},
title = {{Circular Business Models versus circular benefits: An assessment in the waste from Electrical and Electronic Equipments sector}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0959652619318463},
volume = {231},
year = {2019}
}

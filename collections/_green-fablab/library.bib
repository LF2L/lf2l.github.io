pages = {1031--1044},
publisher = {Elsevier Ltd},
title = {{Sharing economy–based service triads: Towards an integrated framework and a research agenda}},
url = {https://doi.org/10.1016/j.jclepro.2019.02.019},
volume = {218},
year = {2019}
}

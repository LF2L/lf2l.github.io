keywords = {Additive manufacturing,Design theory and methodology,Function integration,Part consolidation},
number = {1-4},
pages = {327--342},
title = {{Additive manufacturing-enabled design theory and methodology: a critical review}},
volume = {80},
year = {2015}
}

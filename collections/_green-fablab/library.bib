isbn = {0013936X (ISSN)},
issn = {0013-936X},
journal = {Environ. Sci. Technol.},
month = {oct},
number = {20},
pages = {12044--12053},
pmid = {26402038},
title = {{Emissions of Nanoparticles and Gaseous Material from 3D Printer Operation}},
url = {https://pubs.acs.org/doi/10.1021/acs.est.5b02805},

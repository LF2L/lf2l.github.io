file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Rani et al. - 2019 - Miniaturized Near-Infrared (MicroNIR) Spectrometer in Plastic Waste Sorting.pdf:pdf},
issn = {1996-1944},
journal = {Materials (Basel).},
keywords = {Chemometrics,Circular economy,Near-infrared (NIR) spectroscopy,Partial least squares-discriminant analysis (PLS-D,Principal component analysis (PCA),Urban plastic waste},
month = {aug},
number = {17},
pages = {2740},
publisher = {MDPI AG},
title = {{Miniaturized Near-Infrared (MicroNIR) Spectrometer in Plastic Waste Sorting}},

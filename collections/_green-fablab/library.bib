isbn = {22128271},
issn = {22128271},
journal = {Procedia CIRP},
keywords = {Additive manufacturing,Fused deposition modelling,Genetic algorithm,Internal parametric structures,Optimization},
pages = {800--805},
publisher = {Elsevier B.V.},
title = {{An optimization approach for components built by fused deposition modeling with parametric internal structures}},
url = {http://dx.doi.org/10.1016/j.procir.2014.02.050},
volume = {17},
year = {2014}
}

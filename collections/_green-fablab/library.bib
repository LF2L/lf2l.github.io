pages = {124--127},
publisher = {Elsevier},
title = {{Recycling of polymer waste with SiC/Al2O3 reinforcement for rapid tooling applications}},
url = {https://doi.org/10.1016/j.mtcomm.2018.02.008 https://linkinghub.elsevier.com/retrieve/pii/S2352492817301071},

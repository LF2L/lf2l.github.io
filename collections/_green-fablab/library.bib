journal = {Resour. Conserv. Recycl.},
keywords = {Public perception,Rural China,Rural solid waste,Source-separated collection,Willingness to pay},
pages = {166--173},
publisher = {Elsevier B.V.},
title = {{Public perceptions and economic values of source-separated collection of rural solid waste: A pilot study in China}},
url = {http://dx.doi.org/10.1016/j.resconrec.2015.12.010},
volume = {107},

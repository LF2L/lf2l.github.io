doi = {10.1016/j.jclepro.2020.122072},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Khmara, Kronenberg - 2020 - Degrowth in the context of sustainability transitions In search of a common ground.pdf:pdf},

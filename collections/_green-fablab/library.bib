isbn = {1355254131130},
issn = {1355-2546},
journal = {Rapid Prototyp. J.},
keywords = {open source,polymer extrusion,rapid prototyping,recycling,waste plastic recycling},
month = {mar},
number = {2},
pages = {118--125},
title = {{Distributed recycling of waste polymer into RepRap feedstock}},
url = {http://www.emeraldinsight.com/10.1108/13552541311302978 http://www.emeraldinsight.com/doi/abs/10.1108/13552541311302978 https://www.emerald.com/insight/content/doi/10.1108/13552541311302978/full/html},

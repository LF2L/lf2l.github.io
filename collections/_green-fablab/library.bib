doi = {10.1016/j.resconrec.2020.104882},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Sherman et al. - 2020 - Associate Professor of Epidemiology (Environmental Health Sciences) and Co-Chair, Lancet Countdown Tracking Prog.pdf:pdf},
issn = {09213449},
journal = {Resour. Conserv. Recycl.},
month = {oct},

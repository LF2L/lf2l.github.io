file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Chu et al. - 2016 - From design for manufacturing (DFM) to manufacturing for design (MFD) via hybrid manufacturing and smart factory A r.pdf:pdf},
issn = {21980810},
journal = {Int. J. Precis. Eng. Manuf. - Green Technol.},
keywords = {Design for manufacturing,Hybrid manufacturing,Manufacturing for design,Platform,Smart factory},
number = {2},
pages = {209--222},
title = {{From design for manufacturing (DFM) to manufacturing for design (MFD) via hybrid manufacturing and smart factory: A review and perspective of paradigm shift}},
volume = {3},
year = {2016}

journal = {J. Work. Learn.},
number = {2},
pages = {97--116},
title = {{Spaces of innovation: experiences from two small high‐tech firms}},
url = {http://www.emeraldinsight.com/doi/10.1108/13665621111108774},
volume = {23},
year = {2011}
}

keywords = {3D printing,Additive manufacturing,Circular economy,Distributed recycling,Plastic recycling},
month = {aug},
pages = {121602},
publisher = {Elsevier Ltd},
title = {{Plastic recycling in additive manufacturing: A systematic literature review and opportunities for the circular economy}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0959652620316498},
volume = {264},
year = {2020}
}

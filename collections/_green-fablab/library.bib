issn = {0956053X},
journal = {Waste Manag.},
keywords = {Identification,MIR,Polymer recycling,Sorting,Styrenics,WEEE},
month = {jul},
pages = {513--525},
publisher = {Elsevier Ltd},
title = {{MIR spectral characterization of plastic to enable discrimination in an industrial recycling context: I. Specific case of styrenic polymers}},

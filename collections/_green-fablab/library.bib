keywords = {Engineering design,design and build activities,design thinking,first and second year engineering students,mechanical engineering education},
number = {2},
pages = {148--164},
title = {{Strengthening engineering design skills of first-year university students under resources constraints}},
volume = {44},
year = {2016}
}

keywords = {Cluster,Collaboration,Cooperation,Innovation process,Interorganizational citizenship behavior,Sport},
pages = {55--64},
publisher = {Elsevier Inc.},
title = {{The role of interorganizational citizenship behaviors in the innovation process}},
url = {http://dx.doi.org/10.1016/j.jbusres.2016.12.005},
volume = {73},
year = {2017}
}

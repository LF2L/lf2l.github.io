publisher = {Elsevier Ltd},
title = {{Recycling and remanufacturing of 3D printed continuous carbon fiber reinforced PLA composites}},
url = {http://dx.doi.org/10.1016/j.jclepro.2016.11.139 https://linkinghub.elsevier.com/retrieve/pii/S0959652616320017},
volume = {142},

issn = {14333015},
journal = {Int. J. Adv. Manuf. Technol.},
keywords = {Costing model framework,Fused deposition modeling,Hybrid process planning,Machining},
number = {9-12},
pages = {3091--3108},
publisher = {The International Journal of Advanced Manufacturing Technology},
title = {{A manufacturing cost analysis framework to evaluate machining and fused filament fabrication additive manufacturing approaches}},
volume = {102},
year = {2019}
}

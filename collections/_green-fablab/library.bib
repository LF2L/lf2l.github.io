issn = {00401625},
journal = {Technol. Forecast. Soc. Change},
keywords = {Bio-based economy,Bio-based innovation,Bioeconomy policy,Competing goals dilemma,Input-output analysis,Monte carlo simulation,Sustainability trade-off},
number = {August 2019},
pages = {119946},
publisher = {Elsevier},
title = {{Effective bioeconomy? a MRIO-based socioeconomic and environmental impact assessment of generic sectoral innovations}},
url = {https://doi.org/10.1016/j.techfore.2020.119946},
volume = {153},
year = {2020}
}

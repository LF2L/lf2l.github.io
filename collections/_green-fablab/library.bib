doi = {10.1016/j.tele.2014.05.001},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Kostakis, Niaros, Giotitsas - 2015 - Open source 3D printing as a means of learning An educational experiment in two high schools in Gre.pdf:pdf},
issn = {07365853},
journal = {Telemat. Informatics},
keywords = {3D Printing,Collaboration,Constructionism,Education,Open source},
month = {feb},
number = {1},
pages = {118--128},
publisher = {Elsevier Ltd},
title = {{Open source 3D printing as a means of learning: An educational experiment in two high schools in Greece}},

publisher = {Elsevier Ltd},
title = {{Mechanical properties and molecular structures of virgin and recycled HDPE polymers used in gravity sewer systems}},
url = {http://dx.doi.org/10.1016/j.polymertesting.2015.06.012},
volume = {46},
year = {2015}
